{
  "name": "subway_status",
  "version": "0.1.1",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "litestar-dev": "pipenv run litestar --app api.index:app run -r -p 8000",
    "next-dev": "next dev",
    "dev": "concurrently \"npm run next-dev\" \"npm run litestar-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --config eslint.config.mjs",
    "generate-types": "openapi-typescript http://localhost:8000/schema/openapi.json -o app/types.d.ts"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "next": "^14",
    "postcss": "^8.4.47",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.11.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.6",
    "eslint-config-prettier": "^9.1.0",
    "openapi-typescript": "^7.4.3",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  }
}
